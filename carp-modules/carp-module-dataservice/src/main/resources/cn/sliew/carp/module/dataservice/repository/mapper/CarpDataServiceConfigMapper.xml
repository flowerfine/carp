<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.sliew.carp.module.dataservice.repository.mapper.CarpDataServiceConfigMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.sliew.carp.module.dataservice.repository.entity.CarpDataServiceConfig">
        <result column="id" property="id" />
        <result column="creator" property="creator" />
        <result column="create_time" property="createTime" />
        <result column="editor" property="editor" />
        <result column="update_time" property="updateTime" />
        <result column="group_id" property="groupId" />
        <result column="type" property="type" />
        <result column="name" property="name" />
        <result column="http_path" property="httpPath" />
        <result column="http_method" property="httpMethod" />
        <result column="http_content_type" property="httpContentType" />
        <result column="ds_id" property="dsId" />
        <result column="query_type" property="queryType" />
        <result column="query_script" property="queryScript" />
        <result column="query_page_enabled" property="queryPageEnabled" />
        <result column="query_result_type" property="queryResultType" />
        <result column="status" property="status" />
        <result column="remark" property="remark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        creator,
        create_time,
        editor,
        update_time,
        group_id, `type`, `name`, http_path, http_method, http_content_type, ds_id, query_type, query_script, query_page_enabled, query_result_type, `status`, remark
    </sql>

</mapper>
